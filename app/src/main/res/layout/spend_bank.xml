<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_pengeluaran"
    xmlns:tools="http://schemas.android.com/tools">

    <TextView
        android:id="@+id/input_pengeluaranBank"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pengeluaran"
        android:textColor="@color/color4"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="103dp"/>

    <TextView
        android:id="@+id/bankPengeluaran"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bank"
        android:textColor="@color/color4"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="172dp" />

    <TextView
        android:id="@+id/jenis_bankPengeluaran"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama Bank"
        android:textColor="@color/color4"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="310dp" />

    <EditText
        android:id="@+id/input_jenisbank_pengeluaran"
        android:layout_width="358dp"
        android:layout_height="49dp"
        android:background="@drawable/button_rounded1"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="361dp" />

    <TextView
        android:id="@+id/text_nominalBank_pengeluaran"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nominal"
        android:textColor="@color/color4"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="449dp" />

    <EditText
        android:id="@+id/input_nominalBank_pengeluaran"
        android:layout_width="358dp"
        android:layout_height="49dp"
        android:background="@drawable/button_rounded1"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="503dp" />

    <Button
        android:id="@+id/button_tambah_pengeluaranBank"
        android:layout_width="120dp"
        android:layout_height="49dp"
        android:background="@drawable/button_rounded1"
        android:text="Tambah"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="618dp" />

</androidx.constraintlayout.widget.ConstraintLayout>