<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="16dp"
    android:background="@drawable/bg_total">

    <TextView
        android:id="@+id/view_totalUang"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Total uang"
        android:textColor="@color/color3"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="62dp" />

    <TextView
        android:id="@+id/text_totalCash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cash"
        android:textColor="@color/color4"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="253dp" />

    <TextView
        android:id="@+id/viewTotal_cash"
        android:layout_width="376dp"
        android:layout_height="42dp"
        android:height="35dp"
        android:background="@drawable/button_rounded"
        android:textAlignment="center"
        android:textColor="@color/color3"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="307dp" />

    <TextView
        android:id="@+id/text_Totalbank1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bank"
        android:textColor="@color/color4"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="396dp" />

    <TextView
        android:id="@+id/view_totalBank1"
        android:layout_width="376dp"
        android:layout_height="42dp"
        android:height="35dp"
        android:background="@drawable/button_rounded"
        android:textAlignment="center"
        android:textColor="@color/color3"
        tools:layout_editor_absoluteX="2dp"
        tools:layout_editor_absoluteY="451dp" />

    <TextView
        android:id="@+id/text_Totalbank2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bank"
        android:textColor="@color/color4"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="544dp" />

    <TextView
        android:id="@+id/view_totalBank2"
        android:layout_width="376dp"
        android:layout_height="42dp"
        android:height="35dp"
        android:background="@drawable/button_rounded"
        android:textAlignment="center"
        android:textColor="@color/color3"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="599dp" />

</androidx.constraintlayout.widget.ConstraintLayout>