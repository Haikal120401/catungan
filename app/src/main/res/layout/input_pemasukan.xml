<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color5">

    <TextView
        android:id="@+id/title_pemasukan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pemasukan"
        android:textColor="@color/color4"
        android:textSize="30dp"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="85dp" />

    <TextView
        android:id="@+id/text_jenisTabungan_pemasukan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pilih jenis tabungan"
        android:textColor="@color/color4"
        android:textSize="15dp"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="202dp" />

    <Button
        android:id="@+id/button_pemasukanCash"
        android:layout_width="120dp"
        android:layout_height="49dp"
        android:background="@drawable/button_rounded1"
        android:text="Cash"
        tools:layout_editor_absoluteX="40dp"
        tools:layout_editor_absoluteY="275dp" />

    <Button
        android:id="@+id/button_pemasukanBank"
        android:layout_width="120dp"
        android:layout_height="49dp"
        android:background="@drawable/button_rounded1"
        android:text="Bank"
        tools:layout_editor_absoluteX="255dp"
        tools:layout_editor_absoluteY="275dp" />

</androidx.constraintlayout.widget.ConstraintLayout>