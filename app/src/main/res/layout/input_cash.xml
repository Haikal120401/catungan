<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/bg_pemasukan">

    <TextView
        android:id="@+id/input_pemasukanCash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pemasukan"
        android:textAlignment="center"
        android:textColor="@color/color4"
        android:textSize="30dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="121dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/cashPemasukan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cash"
        android:textColor="@color/color4"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="172dp" />

    <TextView
        android:id="@+id/text_nominalCash_pemasukan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nominal"
        android:textColor="@color/color4"
        android:textSize="20dp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="343dp"/>

    <EditText
        android:id="@+id/Input_nominalCash_pemasukan"
        android:layout_width="358dp"
        android:layout_height="49dp"
        android:background="@drawable/button_rounded1"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="386dp" />

    <Button
        android:id="@+id/button_tambah_pemasukanCash"
        android:layout_width="120dp"
        android:layout_height="49dp"
        android:background="@drawable/button_rounded1"
        android:text="Tambah"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="490dp" />


</androidx.constraintlayout.widget.ConstraintLayout>